<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>CreateLegalCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.CreateLegalCustomerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.CreateRealCustomerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SearchLegalCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.SearchLegalCustomerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SearchRealCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.SearchRealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteLegalCustomerServlet</servlet-name>
