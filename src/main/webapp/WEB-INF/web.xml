    <servlet-name>UpdateLegalCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.UpdateLegalCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateRealCustomerServlet</servlet-name>
    <servlet-class>presentationLayer.UpdateRealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SaveLegalCustomerChangedServlet</servlet-name>
    <servlet-class>presentationLayer.SaveLegalCustomerChangedServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SaveRealCustomerChangedServlet</servlet-name>
    <servlet-class>presentationLayer.SaveRealCustomerChangedServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateLegalCustomerServlet</servlet-name>
    <url-pattern>/CreateLegalCustomerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <url-pattern>/CreateRealCustomerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SearchLegalCustomerServlet</servlet-name>
    <url-pattern>/SearchLegalCustomerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SearchRealCustomerServlet</servlet-name>
    <url-pattern>/SearchRealCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteLegalCustomerServlet</servlet-name>
    <url-pattern>/DeleteLegalCustomerServlet</url-pattern>
  </servlet-mapping>
